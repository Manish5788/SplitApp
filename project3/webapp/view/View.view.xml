<mvc:View
    controllerName="project3.controller.View"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
        		<FlexBox id="flex"
				height="100px"
                width="100px"
				alignContent="Stretch"
                justifyContent="Center"
                
>
            <List
                id="list"
                headerText="list"
                items="{zfosl>/ZFOSL_I_TRAVEL_M}"

            >
                <StandardListItem
                    id="slist"
                    title="{zfosl>travel_id}"
                    description="{zfosl>agency_id}"
                />
            </List>
            	</FlexBox>
        </content>
    </Page>
</mvc:View>
